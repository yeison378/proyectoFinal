<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #242F3D;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ListaActividades">
   <children>
      <TableView fx:id="tabla" layoutX="15.0" layoutY="123.0" prefHeight="200.0" prefWidth="564.0" style="-fx-background-color: #242F3D;" stylesheets="@Lista.css">
        <columns>
          <TableColumn fx:id="actividad" prefWidth="89.0" text="Actividad" />
          <TableColumn fx:id="porcentaje" prefWidth="66.0" text="porcentaje" />
            <TableColumn fx:id="id" prefWidth="70.0" text="id actividad" />
            <TableColumn fx:id="fecha" prefWidth="93.0" text="fecha" />
            <TableColumn fx:id="descripcion" prefWidth="175.0" text="descripcion" />
            <TableColumn fx:id="id_materia" prefWidth="69.0" text="id materia" />
        </columns>
      </TableView>
      <Label fx:id="materia" layoutX="51.0" layoutY="27.0" text="MATERIA" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="cerrar" layoutX="275.0" layoutY="340.0" mnemonicParsing="false" onAction="#cerrarVentana" prefHeight="35.0" prefWidth="89.0" styleClass="botonCancelar" stylesheets="@ActividadStilos.css" text="cerrar" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
