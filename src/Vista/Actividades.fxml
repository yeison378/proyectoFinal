<AnchorPane prefHeight="454.0" prefWidth="330.0" style="-fx-background-color: #242F3D;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Actividad">
