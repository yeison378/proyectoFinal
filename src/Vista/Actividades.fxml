      <Button fx:id="aceptar" layoutX="28.0" layoutY="397.0" mnemonicParsing="false" onAction="#traertexto" prefHeight="37.0" prefWidth="101.0" styleClass="botonAceptar" stylesheets="@ActividadStilos.css" text="ACEPTAR" textFill="WHITE">
