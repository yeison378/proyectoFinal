      <TextArea fx:id="descripcion" layoutX="24.0" layoutY="289.0" prefHeight="97.0" prefWidth="282.0" promptText="Descripcion" />
      <Label layoutX="28.0" layoutY="266.0" prefHeight="17.0" prefWidth="73.0" text="Descripcion" textFill="#f5f5f5" />
      <Label layoutX="57.0" layoutY="166.0" prefHeight="17.0" prefWidth="73.0" text="Porcentaje" textFill="#f5f5f5" />
      <Label layoutX="57.0" layoutY="104.0" prefHeight="17.0" prefWidth="73.0" text="Actividad" textFill="#f5f5f5" />
      <Label layoutX="230.0" layoutY="30.0" text="ID." textFill="WHITE">
