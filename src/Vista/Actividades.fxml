      <Button fx:id="cancelar" layoutX="202.0" layoutY="397.0" mnemonicParsing="false" onAction="#traertexto" prefHeight="37.0" prefWidth="101.0" styleClass="botonCancelar" stylesheets="@ActividadStilos.css" text="CANCELAR" textFill="WHITE">
